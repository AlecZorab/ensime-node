{
  "name": "Ensime",
  "main": "./lib/ensime",
  "version": "0.36.1",
  "description": "Ensime integration for atom",
  "repository": "https://github.com/ensime/ensime-atom",
  "license": "MIT",
  "engines": {
    "atom": ">0.50.0"
  },
  "dependencies": {
    "atom-message-panel": "~1.2.4",
    "atom-package-deps": "~3.0.9",
    "atom-space-pen-views": "^2.1.0",
    "bluebird": "^3.0.6",
    "chokidar": "^1.4.2",
    "diff": "^2.2.1",
    "dom-listener": "^0.1.2",
    "download": "^4.4.3",
    "emissary": "1.x",
    "fs-extra": "^0.26.5",
    "glob": "~6.0.4",
    "jquery": "^2",
    "lodash": "~4.3.0",
    "loglevel": "^1.4.0",
    "recursive-readdir": "^1.3.0",
    "space-pen": "~5.1.2",
    "sub-atom": "~1.0.0",
    "temp": "^0.8.3",
    "vue": "1.0.0-csp"
  },
  "package-deps": [
    "hyperclick",
    "language-scala",
    "linter",
    "browser-plus"
  ],
  "providedServices": {
    "autocomplete.provider": {
      "versions": {
        "2.0.0": "provideAutocomplete"
      }
    },
    "hyperclick.provider": {
      "versions": {
        "0.0.0": "provideHyperclick"
      }
    }
  },
  "consumedServices": {
    "linter-indie": {
      "versions": {
        "1.0.0": "consumeLinter"
      }
    }
  },
  "coffeelintConfig": {
    "indentation": {
      "level": "error",
      "value": 2
    },
    "line_endings": {
      "value": "unix",
      "level": "error"
    },
    "max_line_length": {
      "level": "warn",
      "value": 200
    }
  },
  "devDependencies": {}
}
